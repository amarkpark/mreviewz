<div class="col-xs-10 col-xs-offset-1">
  <h1>Reel Good Movie Reviews</h1>

  <% @movies.each do |m| %>

    <div>
      <h2><%= m.name %></h2>
      <h3>Release Year: <%= m.release_year %></h3>
      <p><%= m.review %></p><br />
      <p><%= m.rating %></p><br />
    </div>
    <br />
    <hr />
    <br />

  <% end %>
</div>
